
func void B_SetAmbientXP()
{
	if(Kapitel == 1)
	{
		XP_Ambient = XP_AmbientKap1;
	}
	else if(Kapitel == 2)
	{
		XP_Ambient = XP_AmbientKap2;
	}
	else if(Kapitel == 3)
	{
		XP_Ambient = XP_AmbientKap3;
	}
	else if(Kapitel == 4)
	{
		XP_Ambient = XP_AmbientKap4;
	}
	else if(Kapitel == 5)
	{
		XP_Ambient = XP_AmbientKap5;
	}
	else if(Kapitel == 6)
	{
		XP_Ambient = XP_AmbientKap6;
	};
};

